      <property name="script">var forms = new java.util.ArrayList()

function resultToXml(rs, xmlElement) {
  var rsmd = rs.getMetaData();
  var result = new XML(xmlElement);

  for (var i = 1; i &lt;= rsmd.getColumnCount(); i++)  {
	var value = rs.getString(i);

    if (value === &quot;null&quot;) {
      value = null;
    }

    result[rsmd.getColumnName(i).toLowerCase()] = value;
  }

  return result;
}

function addOutMigrations() {
  var results = dbConn.executeCachedQuery(&quot;SELECT _URI, VISIT_ID, NAME_OF_DESTINATION, FIELD_WORKER_ID, DATE_OF_MIGRATION, REASON_FOR_OUT_MIGRATION, INDIVIDUAL_ID FROM out_migration_registration_core WHERE VISIT_ID = &apos;&quot; + visitXml[&quot;visit_id&quot;] + &quot;&apos; AND processed_by_mirth is null&quot;);
  addResultsToForms(results, &quot;&lt;outmigration event=&apos;outmigration&apos; table-name=&apos;out_migration_registration_core&apos; /&gt;&quot;);
}

function addResultsToForms(results, xmlElement) {
  while(results.next()) {
    var om = resultToXml(results, xmlElement)
    forms.add(om.toString());
  }
}

function addDeaths() {
  var results = dbConn.executeCachedQuery(&quot;SELECT _URI, VISIT_ID, CAUSE_OF_DEATH, FIELD_WORKER_ID, DATE_OF_DEATH, PLACE_OF_DEATH, INDIVIDUAL_ID FROM death_registration_core WHERE VISIT_ID = &apos;&quot; + visitXml[&quot;visit_id&quot;] + &quot;&apos; AND processed_by_mirth is null&quot;);
  addResultsToForms(results, &quot;&lt;death event=&apos;death&apos; table-name=&apos;death_registration_core&apos;/&gt;&quot;);
}

function addPregnancyObs() {
  var results = dbConn.executeCachedQuery(&quot;SELECT _URI, VISIT_ID, EXPECTED_DELIVERY_DATE, FIELD_WORKER_ID, RECORDED_DATE, INDIVIDUAL_ID FROM pregnancy_observation_core WHERE VISIT_ID = &apos;&quot; + visitXml[&quot;visit_id&quot;] + &quot;&apos; AND processed_by_mirth is null&quot;);
  addResultsToForms(results, &quot;&lt;pregnancyobs event=&apos;pregnancyobs&apos; table-name=&apos;pregnancy_observation_core&apos;/&gt;&quot;);
}

var dbConn = DatabaseConnectionFactory.createDatabaseConnection(&apos;com.mysql.jdbc.Driver&apos;,&apos;jdbc:mysql://localhost/odk_prod&apos;,&apos;root&apos;,&apos;password&apos;);
var visitResult = dbConn.executeCachedQuery(&quot;SELECT _URI, VISIT_DATE, VISIT_ID, LOCATION_ID, FIELD_WORKER_ID, ROUND_NUMBER FROM visit_registration_core WHERE processed_by_mirth is null LIMIT 5&quot;);

while(visitResult.next()) {
  var visitXml = resultToXml(visitResult, &quot;&lt;visit event=&apos;visit&apos; table-name=&apos;visit_registration_core&apos;/&gt;&quot;);
  // does this visit depend on a newly created location ?
  // a visit will not be processed correctly if the location it refers to does not yet exist
  var location = dbConn.executeCachedQuery(&quot;SELECT _URI, HIERARCHY_ID, LOCATION_ID, FIELD_WORKER_ID, LOCATION_TYPE, LOCATION_NAME FROM location_registration_core WHERE LOCATION_ID = &apos;&quot; + visitXml[&apos;location_id&apos;].toString() + &quot;&apos; AND processed_by_mirth is null&quot;);
  
  if (location.next()) {
    var loc = resultToXml(location, &quot;&lt;location event=&apos;location&apos; table-name=&apos;location_registration_core&apos;/&gt;&quot;);
    forms.add(loc.toString());
  }

  forms.add(visitXml.toString());
  
  addDeaths();
  addOutMigrations();
  addPregnancyObs();
}


dbConn.close();
// You may access this result below with $(&apos;column_name&apos;)
return forms;</property>
